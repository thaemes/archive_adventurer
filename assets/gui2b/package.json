{
  "name": "furhat-skill-gui",
  "version": "1.0.0",
  "description": "GUI Template for furhat skill",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "build": "webpack",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "keywords": [
    "es6"
  ],
  "author": "Furhat Robotics",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "furhat-gui": "0.1.0"
  }
}
